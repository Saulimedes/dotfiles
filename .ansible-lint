---
skip_list:
  - 'package-latest' 
  - 'command-instead-of-module' 
  - 'command-instead-of-shell'
  - 'unnamed-task' 
  - 'no-changed-when'
  - yaml[trailing-spaces]  
  - yaml[empty-lines] 
  - var-naming[no-role-prefix] 
  - yaml[comments]
  - yaml[truthy] 
  - latest[git]
  - ignore-errors

warn_list: 
  - 'idempotency' 
  - 'no-changed-when'
  - fqcn[action-core]
  - fqcn[canonical] 

rulesdir: [] 

exclude_paths:
  - '.venv/' 
  - 'tests/' 

quiet: false 
parseable: false 
use_default_rules: true 
